<!DOCTYPE html>
<html>
<head>
  <title>Question Details</title>
</head>
<body>

 
	<section class="signin">
       <div class="row">
          <div class="large-12 columns">
            <div class="main-wrapper">
              <div class="button-group">  
               <% if user_signed_in? != false %>
                  <li class="secondary small button"><%= current_user.email%></li>  

                  <li class="secondary small button"><%= link_to "Sign out", destroy_user_session_path, method: "DELETE"%></li> 
                <%end%>
              </div> 
            </div>
          </div>
        </div>

       
           <script type="text/javascript">
              var map_array = <%= @location_array.inspect %>
              var my_zoom =  <%= @location_zoom %>
   
          </script>
    

            <div id="map_canvas" style="height:70%;width:100%"></div>
          
        

        <div class="row">
          <div class="large-10 large-offset-1 columns blue-box">
            <li><%=@quiz.question%></li>
          </div>
        </div>

        <div class="row">
          <div class="large-6 large-offset-3 columns blue-box">
            <label>Answer
              <input id ="answer" type="text" placeholder="whaddya think" />
            </label>
             <li><%= link_to "CLUE", quiz_path%></li>
              <li><%=@characters.inspect%></li>
          </div>
        </div>
       

      </section>
     
	
</body>
</html>